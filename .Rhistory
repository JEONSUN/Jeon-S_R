library(tidyverse)
mtcars_1 <- mtcars %>%
select(mpg,wt,disp,cyl,am)
# GGally: ggpairs()에 의한 산점도 행렬
# - mtcars의 변수 mpg,wt,disp,cyl,am의 산점도 행렬 작성
# 사용법 : ggpairs(df)
library(GGally)
ggpairs(mtcars_1)
# am,cyl을 요인으로 전환
mtcars_1 <- mtcars %>%
select(mpg,wt,disp,factor(cyl),factor(am))
mtcars_1 <- mtcars %>%
select(mpg,wt,disp,cyl,am)
# am,cyl을 요인으로 전환
mtcars_2 <- mtcars_1 %>%
mutate(cyl=factor(cyl),am=factor(am))
ggpairs(mtcars_2)
